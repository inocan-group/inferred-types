{
  "name": "inferred-types",
  "version": "0.37.6",
  "description": "Functions which provide useful type inference on TS projects",
  "license": "MIT",
  "author": "Ken Snyder<ken@ken.net>",
  "type": "module",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "url": "https://github.com/inocan-group/inferred-types"
  },
  "sideEffects": false,
  "scripts": {
    "about": "tokei src",
    "build": "run-s lint build:bundle",
    "build:bundle": "tsup src/index.ts --dts --format=esm --clean",
    "diagnostics": "tsc -b -f -diagnostics",
    "clean": "rimraf dist/**/*",
    "lint": "run-p lint:*",
    "lint:test": "eslint tests/**/*.ts --fix",
    "lint:src": "eslint src/**/*.ts --fix ",
    "lint:types": "npx tsc --noEmit --diagnostics",
    "trace": "npx tsc --noEmit --diagnostics && npx analyze-trace trace",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run",
    "audit:fix": "pnpm audit --fix",
    "release": "run-s lint release:latest test:ci audit:fix release:bump",
    "release:latest": "pnpm install",
    "release:bump": "bumpp",
    "prepublishOnly": "npm run build",
    "watch": "run-p watch:*",
    "watch:lint": "esw (src|tests)/**/*.ts -w --clear",
    "watch:bundle": "tsup src/index.ts --format=esm --dts"
  },
  "devDependencies": {
    "@type-challenges/utils": "~0.1.1",
    "@types/node": "^16.18.12",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@typescript/analyze-trace": "^0.9.1",
    "@vitest/ui": "^0.28.4",
    "bumpp": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-watch": "^8.0.0",
    "npm-run-all": "~4.1.5",
    "pathe": "^1.1.0",
    "prettier": "~2.8.4",
    "tsup": "^6.6.0",
    "typescript": "5.0.0-dev.20230210",
    "vite": "^4.1.1",
    "vite-node": "^0.28.4",
    "vitest": "^0.28.4",
    "vue": "^3.2.47"
  }
}
