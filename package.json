{
  "name": "inferred-types",
  "version": "0.18.0",
  "description": "Functions which provide useful type inference on TS projects",
  "license": "MIT",
  "author": "Ken Snyder<ken@ken.net>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "about": "tokei src",
    "autoindex": "npx dd autoindex",
    "build": "run-s clean autoindex lint build:bundle",
    "build:force": "run-s clean autoindex build:bundle",
    "build:bundle": "npx tsup src/index.ts --dts --format='esm,cjs'",
    "clean": "rimraf dist/**/*",
    "lint": "eslint src/**/*.ts --fix && tsc --noEmit ",
    "lint:full": "eslint src/**/*.ts && eslint test/**/*.ts && tsc --noEmit",
    "test": "jest"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.11.0",
    "@type-challenges/utils": "~0.1.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^14.17.27",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "common-types": "^1.30.0",
    "cross-env": "^7.0.3",
    "dd": "^0.14.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "jest": "^27.3.1",
    "jest-extended": "~1.2.0",
    "npm-run-all": "~4.1.5",
    "prettier": "~2.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.1",
    "tsup": "^5.10.1",
    "typescript": "^4.4.4"
  },
  "optionalDependencies": {
    "common-types": "^1.30.0"
  },
  "dependencies": {
    "brilliant-errors": "^0.4.0",
    "native-dash": "^1.20.1"
  }
}
