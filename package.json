{
  "name": "inferred-types",
  "private": true,
  "description": "primitives for advanced inference in TS",
  "license": "MIT",
  "type": "module",
  "author": "Ken Snyder<ken@ken.net>",
  "repository": {
    "url": "https://github.com/inocan-group/inferred-types"
  },
  "sideEffects": false,
  "scripts": {
    "prepare": "ts-patch install -s",
    "build": "turbo build",
    "diag": "tsc -b -f --extendedDiagnostics src/inferred-types",
    "diag:constants": "tsc -b -f --extendedDiagnostics src/constants",
    "diag:types": "tsc -b -f --extendedDiagnostics src/types",
    "diag:runtime": "tsc -b -f --extendedDiagnostics src/runtime",
    "clean": "rimraf dist/**/*",
    "lint": "pnpm --filter \"./app/*\" lint",
    "trace": "npx tsc --noEmit --diagnostics && npx analyze-trace trace",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "bench": "cd benches && tsx ./index.ts",
    "test:ci": "vitest run",
    "test:types": "typed test -w 6133 -w 6307",
    "audit:fix": "pnpm audit --fix",
    "audit:types": "typed source -c src/types/tsconfig.json -w 6133 -w 6307",
    "audit:runtime": "typed source -c src/runtime/tsconfig.json -w 2304 -w 6133 -w 6307",
    "audit:constants": "ts-type-tester src/constants src/constants/tsconfig.json --warn 6133",
    "release:latest": "pnpm install",
    "bump": "pnpm ---filter inferred-types bump",
    "prepublishOnly": "npm run build",
    "watch": "run-p watch:*",
    "watch:types": "ts-type-tester tests tests/tsconfig.json --ignore 6133 -w",
    "watch:lint": "esw (src|tests)/**/*.ts -w --clear",
    "watch:bundle": "tsup src/index.ts --format=esm --dts",
    "build:constants": "pnpm --filter constants build",
    "build:types": "pnpm --filter types build",
    "build:runtime": "pnpm --filter runtime build",
    "build:inferred": "pnpm --filter inferred-types build",
    "release": "run-s lint build bump"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@type-challenges/utils": "~0.1.1",
    "@types/node": "^20.17.6",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@typescript/analyze-trace": "^0.10.1",
    "@vitest/ui": "^2.1.4",
    "bumpp": "^9.8.1",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "glob": "^11.0.0",
    "npm-run-all": "~4.1.5",
    "pathe": "^1.1.2",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-patch": "^3.2.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "turbo": "^2.2.3",
    "typecheck": "^0.1.2",
    "typed-tester": "^0.6.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "typescript-transform-paths": "^3.5.1",
    "typescript-transformer-esm": "^1.1.0",
    "vite": "^5.4.10",
    "vite-node": "^2.1.4",
    "vitest": "^2.1.4",
    "vue": "^3.5.12"
  },
  "pnpm": {
    "overrides": {
      "tar@<6.2.1": ">=6.2.1",
      "braces@<3.0.3": ">=3.0.3",
      "micromatch@<4.0.8": ">=4.0.8",
      "vite@>=5.4.0 <=5.4.5": ">=5.4.6"
    }
  },
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=20"
  }
}
