{
  "name": "inferred-types",
  "version": "0.17.0",
  "description": "Functions which provide useful type inference on TS projects",
  "license": "MIT",
  "author": "Ken Snyder<ken@ken.net>",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "typings": "dist/types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prepare": "ts-patch install -s",
    "about": "tokei src",
    "autoindex": "npx dd autoindex",
    "build": "run-s clean autoindex lint build:bundle",
    "build:force": "run-s clean autoindex build:bundle",
    "build:bundle": "node devops/build esnext commonjs --minimize",
    "clean": "rimraf dist/**/*",
    "lint": "eslint src/**/*.ts --fix && tsc --noEmit ",
    "lint:full": "eslint src/**/*.ts && eslint test/**/*.ts && tsc --noEmit",
    "test": "jest"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.27.0",
    "@antfu/eslint-config-ts": "^0.10.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@type-challenges/utils": "~0.1.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^14.17.27",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "common-types": "^1.25.0",
    "cross-env": "^7.0.3",
    "dd": "^0.14.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "jest": "^27.3.1",
    "jest-extended": "~1.1.0",
    "npm-run-all": "~4.1.5",
    "prettier": "~2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.1",
    "ts-patch": "^1.4.4",
    "typescript": "^4.4.4",
    "typescript-transform-paths": "^3.3.1"
  },
  "optionalDependencies": {
    "common-types": "^1.29.1"
  },
  "dependencies": {
    "brilliant-errors": "^0.4.0",
    "native-dash": "^1.20.1"
  }
