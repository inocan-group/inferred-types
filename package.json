{
  "name": "inferred-types",
  "version": "0.37.6",
  "description": "Functions which provide useful type inference on TS projects",
  "license": "MIT",
  "author": "Ken Snyder<ken@ken.net>",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "repository": {
    "url": "https://github.com/inocan-group/inferred-types"
  },
  "sideEffects": false,
  "scripts": {
    "about": "tokei src",
    "build": "run-s lint build:bundle",
    "build:bundle": "tsup src/index.ts --dts --format=esm --clean",
    "watch": "run-p watch:*",
    "watch:bundle": "tsup src/index.ts --dts --format=esm --watch",
    "clean": "rimraf dist/**/*",
    "lint": "eslint src/**/*.ts --fix && tsc --noEmit ",
    "lint:full": "eslint src/**/*.ts && eslint tests/**/*.ts && tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run",
    "audit:fix": "pnpm audit --fix",
    "release": "run-s lint:full release:latest test:ci audit:fix release:bump",
    "release:latest": "pnpm install",
    "release:bump": "bumpp",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "brilliant-errors": "^0.7.3"
  },
  "devDependencies": {
    "@type-challenges/utils": "~0.1.1",
    "@types/node": "^16.18.7",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vitest/ui": "^0.25.6",
    "bumpp": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "npm-run-all": "~4.1.5",
    "pathe": "^1.0.0",
    "prettier": "~2.8.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vitest": "^0.25.6"
  }
}
