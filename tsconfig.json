{
  "compilerOptions": {
    "noEmit": true,
    "module": "ES2022",
    "moduleResolution": "bundler",
    "target": "ES2022",
    "incremental": true,
    "noUnusedLocals": false,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "tsBuildInfoFile": ".tsbuildinfo",
    "strictNullChecks": true,
    "esModuleInterop": true,
    "rootDir": "..",
    "outDir": "../../dist/none",
    "paths": {

    }
  },
  "include": [
    ".",
    "../modules",
    ".claude/hooks"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}
