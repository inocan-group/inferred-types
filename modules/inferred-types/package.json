        "watch:test": "vitest"
    },
    "dependencies": {
        "@inferred-types/constants": "workspace:*",
        "@inferred-types/types": "workspace:*",
        "@inferred-types/runtime": "workspace:*"
    },
    "devDependencies": {
        "@type-challenges/utils": "~0.1.1",
        "npm-run-all": "~4.1.5",
        "tsdown": "^0.12.7"
    }
