{
    "name": "@inferred-types/inferred-types",
    "private": true,
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "scripts": {
