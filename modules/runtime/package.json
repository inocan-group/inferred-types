{
    "name": "@inferred-types/runtime",
    "private": true,
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "scripts": {
        "nuke": "rm -rf node_modules",
        "build": "tsdown",
        "test": "vitest run",
        "lint": "eslint src",
        "circular": "npx madge --circular --warning --exclude '^\\.\\./\\.\\./' --ts-config tsconfig.tsdown.json src/**/*.ts",
        "circular:summary": "FORCE_COLOR=1 npx madge --circular --summary --exclude '^\\.\\./\\.\\./' --ts-config tsconfig.tsdown.json src/**/*.ts | grep -vE '^\\x1b\\[[0-9;]*m\\x1b\\[[0-9;]*m[01]\\x1b'",
        "watch": "run-p watch:*",
        "watch:build": "tsdown --watch",
        "watch:test": "vitest",
        "source": "echo 'Runtime Module' && npx typed source"
    },
    "dependencies": {
        "@inferred-types/constants": "workspace:*",
        "@inferred-types/types": "workspace:*"
    },
    "devDependencies": {
        "@type-challenges/utils": "catalog:",
        "@types/luxon": "catalog:",
        "luxon": "catalog:",
        "moment": "catalog:",
        "npm-run-all": "catalog:",
        "tsdown": "catalog:",
        "typecheck": "catalog:",
        "typed-tester": "catalog:",
        "typescript": "catalog:",
        "vitest": "catalog:",
        "vue": "catalog:"
    }
}
