{
  "extends": "../ts-base.json",
  "references": [
    {
      "path": "../src/constants"
    },
    {
      "path": "../src/runtime"
    },
    {
      "path": "../src/types"
    }
  ],
  "compilerOptions": {
    "noEmit": true,
    "incremental": true,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "tsBuildInfoFile": ".tsbuildinfo",
    "rootDir": "..",
    "baseUrl": ".",
    "outDir": "../../dist/none",
    "paths": {
      "inferred-types": [
        "../src/types/index.ts"
      ],
      "src/constants/index": [
        "../src/constants/index.ts"
      ],
      "inferred-types": [
        "../src/runtime/index.ts"
      ],
      "src/inferred-types/index": [
        "../src/inferred-types/index.ts"
      ],
      "inferred-types/dist/types/index": [
        "../src/types/index.ts"
      ],
      "inferred-types/dist/runtime/index": [
        "../src/runtime/index.ts"
      ],
      "inferred-types": [
        "../src/constants/index.ts"
      ],
      "inferred-types": [
        "../src/inferred-types/index.ts"
      ],
    }
  },
  "include": [
    ".",
    "../src",
  ],
  "exclude": [
    "**/node_modules/*",
  ]
}
