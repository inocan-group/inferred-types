{
    "references": [
        {
            "path": "../constants"
        },
        {
            "path": "../types"
        }
    ],
    "compilerOptions": {
        "strict": true,
        "skipLibCheck": true,
        "module": "ES2022",
        "target": "ES2022",
        "moduleResolution": "node",
        "declaration": true,
        "declarationMap": true,
        "baseUrl": ".",
        "rootDir": "../..",
        "outDir": "../../dist/runtime",
        "composite": true,
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "src/runtime": [
                "./index.ts"
            ],
            "src/types": [
                "../types/index.ts"
            ],
            "src/constants": [
                "../constants/index.ts"
            ],
            "src/runtime/*": [
                "./*"
            ],
        },
    },
    "include": [
        "index.ts",
        "**/*.ts",
    ],
    "exclude": [
        "./boolean-logic/ifTypeOf.ts"
    ]
}
