{
    "references": [
        {
            "path": "../constants"
        },
        {
            "path": "../types"
        },
        {
            "path": "../runtime"
        },
    ],
    "compilerOptions": {
        "strict": true,
        "skipLibCheck": true,
        "module": "ES2022",
        "target": "ES2022",
        "moduleResolution": "node",
        "declaration": true,
        "declarationMap": true,
        "baseUrl": ".",
        "rootDir": "../..",
        "outDir": "./dist",
        "composite": true,
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "src/runtime": [
                "../runtime/index.ts"
            ],
            "src/types": [
                "../types/index.ts"
            ],
            "src/constants": [
                "../constants/index.ts"
            ],
            "./*": [
                "./*"
            ],
        },
    }
}
